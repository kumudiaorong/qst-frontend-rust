{
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "c": true,
        "go": true
    },
    "doxdocgen.generic.authorName": "kumudiaorong",
    "doxdocgen.generic.authorEmail": "kmdr.error@gmail.com",
    "window.zoomLevel": 1,
    "protoc": {
        "path": "protoc",
        "compile_on_save": false,
        "options": [
            "--proto_path=protos/v3",
            "--proto_path=protos/v2",
            "--proto_path=${workspaceRoot}/qst-proto/src",
            "--proto_path=${env.GOPATH}/src",
        ]
    },
    "rust-analyzer.linkedProjects": [
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml"
    ]
}